{
  "name": "cognito-express",
  "version": "2.0.17",
  "description": "cognito-express authenticates API requests on a Node-Express application by verifying the signature of AccessToken or IDToken generated by Amazon Cognito.",
  "main": "./lib/index.js",
  "scripts": {
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --timeout 10000 -r ts-node/register 'test/**/*.ts'",
    "start": "npm test",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "keywords": [
    "express",
    "aws",
    "amazon",
    "passport",
    "cognito",
    "auth",
    "authentication",
    "identity"
  ],
  "author": "Gary Arora (https://twitter.com/AroraGary)",
  "license": "MIT",
  "files": [
    "LICENSE",
    "README.md",
    "lib/"
  ],
  "dependencies": {
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/jwk-to-pem": "^2.0.0",
    "@types/mocha": "^8.2.0",
    "@types/request-promise": "^4.1.47",
    "@types/rewire": "^2.5.28",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "istanbul": "^0.4.5",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "rewire": "^5.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghdna/cognito-express.git"
  },
  "bugs": {
    "url": "https://github.com/ghdna/cognito-express/issues"
  },
  "homepage": "https://github.com/ghdna/cognito-express#readme"
}
